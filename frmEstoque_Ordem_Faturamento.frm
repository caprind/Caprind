                If PIS_Prod <> 0 Then TBProduto!Total_PIS_prod = Format((Valor_total * PIS_Prod) / 100, "###,##0.00") Else TBProduto!Total_PIS_prod = 0
            Else
                TBProduto!PIS_Prod = 0
