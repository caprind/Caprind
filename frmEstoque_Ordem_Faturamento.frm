    If USMsgBox("A data de entrada não foi informada, deseja utilizar a data de hoje?", vbYesNo, "CAPRIND v5.0") = vbYes Then
        txt_EntSai = Format(Date, "dd/mm/yyyy")
    Else
        NomeCampo = "a data de entrada"
        ProcVerificaAcao
        FunChecaCampos = False
        txt_EntSai.SetFocus
        Exit Function
    End If
End If
