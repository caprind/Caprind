            End If
            If TBProduto!CST_Cofins = "01" Or TBProduto!CST_Cofins = "02" Or TBProduto!CST_Cofins = "03" Or TBProduto!CST_Cofins = "49" Or TBProduto!CST_Cofins = "98" Or TBProduto!CST_Cofins = "99" Then
