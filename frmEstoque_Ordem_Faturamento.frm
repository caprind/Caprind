   End If
   
    TBTotaisnota!dbl_Valor_Frete = IIf(txt_VlrFrete = "", 0, txt_VlrFrete)
    TBTotaisnota!dbl_Valor_Seguro = IIf(txt_vlrSeguro = "", 0, txt_vlrSeguro)
